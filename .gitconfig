[core]
    repositoryformatversion = 0
    filemode = false
    bare = true
    symlinks = false
    ignorecase = true
    hideDotFiles = dotGitOnly
[remote "sp"]
    url = git@github.com:shortpoet/sp.git
    fetch = +refs/heads/*:refs/heads/*
    fetch = +refs/tags/*:refs/tags/*
    mirror = true
[remote "mirrors"]
    url = git@ssh.dev.azure.com:v3/shortpoet/Shortpoet/Shortpoet
    mirror = true
    skipDefaultUpdate = true
